{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "src/api/*.ts",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/demo/:path*",
      "destination": "/demo/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}